{
  "name": "song-builder",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "VITE_USE_MOCK_LLM=true vite",
    "build": "tsc -p tsconfig.json && vite build",
    "lint": "eslint src",
    "test": "playwright test",
    "generate:fixtures": "npx tsx scripts/generate-fixtures.ts",
    "screenshot:home": "playwright test --grep @screenshot:home",
    "screenshot:lyrics": "playwright test --grep @screenshot:lyrics",
    "screenshot:lyrics-list": "playwright test --grep @screenshot:lyrics-list",
    "screenshot:checkpoint": "playwright test --grep @screenshot:checkpoint",
    "screenshot:songs": "playwright test --grep @screenshot:songs",
    "screenshot:pinned": "playwright test --grep @screenshot:pinned",
    "screenshot:settings": "playwright test --grep @screenshot:settings"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.564.0",
    "openai": "^4.77.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^7.3.1"
  }
}
